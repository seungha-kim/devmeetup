{
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.js --inline --hot --https --content-base src/",
    "test": "tape --require ./tape-preload src/**/*.spec.js | tap-notify | tap-spec",
    "test:watch": "nodemon --watch ./src --ext js --exec \"npm test\"",
    "test:coverage": "isparta cover ./node_modules/tape/bin/tape -- --require ./tape-preload src/**/*.spec.js",
    "clean": "rimraf dist",
    "build": "webpack --config webpack/prod.js  --progress",
    "deploy": "npm run clean && npm run build && node bin/deploy.js",
    "serve": "npm run clean && npm run build && firebase serve"
  },
  "dependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.9.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^3.2.0",
    "firebase-tools": "^3.0.4",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^4.0.0",
    "immutable": "^3.8.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "node-sass": "^3.8.0",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.1",
    "redux-saga": "^0.11.0",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "tape": "^4.6.0",
    "tone": "^0.7.1",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-react": "^2.5.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "nodemon": "^1.9.2",
    "react-hot-loader": "^1.3.0",
    "tap-notify": "^1.0.0",
    "tap-spec": "^4.1.1",
    "webpack-dev-server": "^1.14.1"
  }
}
